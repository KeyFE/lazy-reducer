{
  "name": "lazy-reducer",
  "version": "0.4.2",
  "description": "lazy reducer",
  "main": "lib/index.js",
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:umd": "webpack",
