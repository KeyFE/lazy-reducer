    "clean": "rimraf lib dist",
    "rebuild": "npm run clean && npm run build",
    "prepublishOnly": "npm run clean && npm run rebuild",
    "test": "npm run rebuild && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KeyFE/lazy-reducer.git"
  },
